{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "prebuild": "ruby -v && echo 'Ruby installed' && bundler -v && echo 'Bundler installed' || echo 'Bundler not installed'",
  "build": "npm run prebuild && bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "reset": "npx rimraf .git",
    "start": "bundle exec jekyll serve --livereload",
    "federalist": "npm run build",
    "test": "bundle exec htmlproofer ./_site --check-html --disable-external; npx a11y './_site/**/*.html'"
  },
  "dependencies": {
    "netlify-cms": "^2.10.102",
    "uswds": "^2.14.0"
  },
  "devDependencies": {
    "a11y": "^0.5.1",
    "rimraf": "^3.0.2"
  }
}
